<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Contact Us</h2>
                <h3 class="section-subheading text-muted">Use the below form to Contact Donald Boulton.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <form id="contact" method="post">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="name" class="sr-only">Your Name</label>
                                <input class="form-control is-valid" id="name" name="name" type="text" placeholder="Your Name *" required="" data-validation-required-message="Please enter your name.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <label for="Email" class="sr-only">Your Email</label>
                                <input class="form-control is-valid" id="email" name="email" type="email" placeholder="Your Email *" required="" data-validation-required-message="Please enter your email address.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <label for="Email" class="sr-only">Your Email</label>
                                <input class="form-control is-valid" id="confirmEmail" name="confirmEmail" type="email" placeholder="Confirm Email *" required="" data-validation-required-message="Please confirm your email address.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <label for="Subject" class="sr-only">Subject of your Message</label>
                                <input class="form-control" id="subject" name="subject" type="text" placeholder="Subject *" required="" data-validation-required-message="Please enter your subject.">
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="Message" class="sr-only">Your Message</label>
                                <textarea class="form-control is-valid" id="message" name="message" placeholder="Your Message *" required="" data-validation-required-message="Please enter a message."></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <input id="ReturnUrl" name="ReturnUrl" type="hidden" value="" />
                    <input id="Id" name="Id" type="hidden" value="" />
                    <div class="g-recaptcha"
                         data-sitekey="6Le_8DAUAAAAAC2B902PFAI_HpazMMZ6vhTkaTAW"
                         data-callback="onSubmit"
                         data-size="invisible">
                    </div>
                    <div class="col-lg-12 text-center">
                        <button id="sendMessage" class="g-recaptcha invisible-recaptcha btn btn-primary btn-xl text-uppercase" data-sitekey="6Le_8DAUAAAAAC2B902PFAI_HpazMMZ6vhTkaTAW" data-callback="submit" name="sendMessage" type="submit"><i class="fas fa-paper-plane"></i>&nbsp;Send</button>
                    </div>
                    <label class="sr-only">Keep this field blank</label>
                    <input id="honeypot" type="text" name="honeypot" value="" hidden>
                </form>
            </div>
        </div>
        <div id="thankyou_message" hidden>
            <h2>
                <em>Thanks</em> for contacting us!
                We will get back to you soon!
            </h2>
        </div>
        <div id="success"></div>
    </div>
</section>

