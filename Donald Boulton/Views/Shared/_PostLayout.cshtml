                            <div class="dropdown-divider"></div>
                            Blogs &nbsp;&nbsp;<i class="fas fa-archive"></i>
                            <a class="dropdown-item" href="/Blog/Blog">Blog</a>
                            <a class="dropdown-item" href="/Post">Core 2 Posts</a>
                            <div class="dropdown-divider"></div>
                            Other Blogs &nbsp;&nbsp;<i class="fas fa-external-link-alt"></i>
                            <a class="dropdown-item" href="http://bibwoe.com">Bibwoe</a>
                            <a class="dropdown-item" href="http://mansbooks.com">Mansbooks</a>
                            <a class="dropdown-item" href="http://publiuslogic.com">Publiuslogic</a>
                        </div>
                    </li>
                </ul>
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown1" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="fas fa-sign-in-alt"></i> </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown1">
                            <div id="login-dropdown" class="group">
                                <div id="Login">
                                    <div class="user-actions">
                                        <div>
                                            @await Html.PartialAsync("_LoginPartial")
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </li>
                </ul>
                <div>
                    <div>
                        <form class="form-inline my-2 my-lg-0 search-form" role="form" method="GET" action="/Search">
                            <div class="form-group input-group"><input id="q" name="q" type="text" value="" class="form-control mr-sm-2" placeholder="Search" /> <button type="submit" class="btn btn-primary"> <i class="fas fa-search"></i> </button></div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <section id="about">
        <div class="container">
            <div class="body-content">
                <section>
                    <div class=" clearfix"></div>
                    <div><label></label></div>
                    <div class=" clearfix"></div>
                    <div class="col-md-8">
                        <div class="row">
                            @await Component.InvokeAsync("ArticleCarousel")
                        </div>
                    </div>
                </section>
                <div id="body" data-pjax-container>
                    @RenderBody()
                </div>
                <div id="google-plus">
                    <div class="container">
                        <div class="g-plus" data-action="share" data-href="https://donboulton.com"></div>
                        <div id="sharePost"></div>
                        <script>
                            var options = {
                                contenturl: 'https://plus.google.com/pages/',
                                contentdeeplinkid: '/pages',
                                clientid: 'xAIzaSyC4KP1dVsMy4zV7Ahi9zRn1c03sTR0zc0g.apps.googleusercontent.com',
                                cookiepolicy: 'single_host_origin',
                                prefilltext: 'Create your Google+ Page too!',
                                calltoactionlabel: 'CREATE',
                                calltoactionurl: 'http://plus.google.com/pages/create',
                                calltoactiondeeplinkid: '/pages/create'
                            };
                            // Call the render method when appropriate within your app to display
                            // the button.
                            gapi.interactivepost.render('sharePost', options);
                        </script>
                        <div id="comments"></div>
                        <script>
                            window.onLoadCallback = function () {
                                gapi.comments.render('comments', {
                                    href: window.location,
                                    width: '624',
                                    first_party_property: 'Blogger',
                                    view_type: 'FILTERED_POSTMOD'
                                });
                            }
                        </script>
                    </div>
                </div>
                <footer>
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-4 text-center">
                                <span id="Copyright" class="Copyright">&copy; 2018 @@ <a data-rel="author" itemprop="url" data-toggle="tooltip" data-placement="top" href="https://plus.google.com/117122847504916016102?rel=author" rel="me" data-user-id="117122847504916016102" data-origional-title="Donald Boulton on Google + Posts and Discussions."><span itemprop="name">Donald Boulton</span></a></span>
                            </div>
                            <div class="col-md-4">
                                <ul class="list-inline social-buttons text-center">
                                    <li class="list-inline-item"><a class="list-inline-item" data-toggle="tooltip" data-count="horizontal" href="https://twitter.com/intent/tweet?url=https://donboulton.com/&amp;text=DonaldBoulton&amp;via=donboulton" data-original-title="Twitter Share" data-user-id="105217183" target="_blank"> <i class="fab fa-twitter"></i> </a></li>
                                    <li class="list-inline-item"><a data-toggle="tooltip" data-count="horizontal" data-original-title="My Github Projects, get involved in building the Books Of Life!" target="_blank" href="https://github.com/donaldboulton"> <i class="fab fa-github"></i> </a></li>
                                    <li class="list-inline-item"><a data-toggle="tooltip" data-original-title="Mansbooks Slack, Join the team." data-count="horizontal" target="_blank" href="https://mansbooks.slack.com"> <i class="fab fa-slack"></i> </a></li>
                                </ul>
                            </div>
                            <div class="col-md-4">
                                <ul class="list-inline quicklinks text-center">
                                    <li class="list-inline-item"><a itemprop="url" data-toggle="tooltip" data-placement="top" data-origional-title="Privacy and Cookie Usage for Donald Boulton's Web Sites'" href="/Contact/Privacy">Privacy Policy</a></li>
                                    <li class="list-inline-item"><a itemprop="url" data-toggle="tooltip" data-placement="top" data-origional-title="Terms of Use" href="/Contact/Terms">Terms of Use</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </section>
    <environment include="Development">
        <script src="https://d3n8k0cqgcsxxu.cloudfront.net/js/jquery.min.js">
